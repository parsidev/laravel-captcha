{
    "name": "parsidev/laravel-captcha",
    "description": "Captcha integration for Laravel",
    "keywords": ["captcha", "laravel captcha"],
    "license": "MIT",
    "homepage": "https://github.com/parsidev/laravel-captcha",
    "authors": [
        {
            "name": "Dmitriy Igoshev",
            "email": "dmitry.igo@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/parsidev/laravel-captcha/issues",
        "source": "https://github.com/parsidev/laravel-captcha"
    },
    "require": {
        "php": "^8.1",
        "ext-gd": "*",
        "illuminate/support": "^10.0",
        "illuminate/http": "^10.0",
        "illuminate/routing": "^10.0",
        "illuminate/view": "^10.0",
        "illuminate/session": "^10.0",
        "illuminate/config": "^10.0",
        "illuminate/translation": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Parsidev\\Captcha\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.2-dev"
        },
        "laravel": {
            "providers": [
                "Parsidev\\Captcha\\Providers\\CaptchaServiceProvider"
            ]
        }
    }
}
